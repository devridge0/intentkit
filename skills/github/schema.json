{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "GitHub Skills",
  "description": "Configuration schema for GitHub skills",
  "properties": {
    "states": {
      "type": "object",
      "properties": {
        "github_search": {
          "type": "string",
          "title": "GitHub Search",
          "enum": [
            "disabled",
            "public",
            "private"
          ],
          "description": "State of the github_search skill"
        }
      },
      "description": "States for each GitHub skill (disabled, public, or private)"
    }
  },
  "required": ["states"],
  "additionalProperties": true
} 